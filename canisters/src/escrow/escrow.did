type RejectionCode = variant {
  NoError;
  CanisterError;
  SysTransient;
  DestinationInvalid;
  Unknown;
  SysFatal;
  CanisterReject;
};
type Result = variant { Ok : nat; Err : record { RejectionCode; text } };
service : {
  getPot : (nat64) -> (Result);
  get_caller_balance : () -> (Result);
  get_canister_balance : () -> (Result);
  get_deposit_address : (nat64) -> (vec nat8);
  get_value : () -> (text);
  place_bet : (nat64, nat64) -> (Result);
}
